{  
  "$schema": "http://json-schema.org/draft-07/schema#",  
  "type": "object",  
  "properties": {  
    "vehicles": {  
      "type": "array",  
      "minItems": 1,  
      "items": {  
        "type": "object",  
        "properties": {  
          "gosNumber": {  
            "type": "string"  
          },  
          "depotNumber": {  
            "type": "integer"  
          },  
          "is_active": {  
            "type": "boolean"  
          }  
        },  
        "required": ["gosNumber", "depotNumber", "is_active"]  
      }  
    }  
  },  
  "required": ["vehicles"]  
}  
